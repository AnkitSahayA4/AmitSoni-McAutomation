@using IARTAutomationApp.Models

@model  IARTAutomationApp.ViewModels.EmployeeAll

@{
    ViewBag.Title = "Reports";

    string qualification = "", BloodGroup="", BloodGenotype="", address = "", StateOfOrigin="", LGA="", MobileNo="", EmpEmailId="";

    string BankType = "", NameOfBanks = "", BankBranch = "", AccountNumber = "", AccountName = "", PFA = "", RSAPinNo = "", SalaryScale = "",NhisNo="",NhisProvider="",Bloodgroup="",Genotype="" ;


}
<!-- Content Wrapper. Contains page content -->
<!-- Main content -->
<div class="content">

    <ol class="breadcrumb bc-colored bg-success m-b-3">
        <li class="breadcrumb-item"><a href="#">Reports</a></li>
        <li class="breadcrumb-item active">Employees Personal Information Report</li>
    </ol>

    <div class="card">
        <div class="card-body">
            <h4 class="text-black">Employees Personal Information Report</h4>
            <p>Export data to Copy, CSV, Excel & PDF</p>
            <div class="table-responsive">
                <table id="emppi" class="table table-bordered table-hover" data-name="cool-table">
                    <thead>
                        <tr>
                            <th>File No</th>
                            <th>Employee Code</th>
                            <th>Title</th>
                            <th>First Name</th>
                            <th>MiddleName</th>
                            <th>SurName</th>
                            <th>Sex</th>
                            <th>Qualification</th>
                            <th>Date Of Birth</th>
                            <th>Place Of Birth</th>
                            <th>Marital Status</th>
                            <th>Maiden Name</th>
                            <th>Spouse Name</th>
                            <th>State Of Origin</th>
                            <th>LGA Of Origin</th>
                            <th>Home Town</th>
                            <th>Religion</th>
                            <th>Blood Group</th>
                            <th>Geno Type</th>
                            <th>Contact Home Address</th>
                            <th>Last Promotion</th>
                            <th>Permanent Home Address</th>
                            <th>State Of Residence</th>
                            <th>LGA of Residence</th>
                            <th>Mobile No</th>
                            <th>Email</th>
                            <th>First Appointment</th>
                            <th>Date of First Appointment</th>
                            <th>Confirmation Date</th>
                            <th>Last Promotion Date</th>
                            <th>Job Town</th>
                            <th>Bank Type</th>
                            <th>Bank Name</th>
                            <th>Bank Branch</th>
                            <th>Account Number</th>
                            <th>Bank Account Name</th>
                            <th>Pension Name</th>
                            <th>Pension Pin</th>
                            <th>Cadre/Job Schedule</th>
                            <th>Programme</th>
                            <th>Unit</th>
                            <th>Section</th>
                            <th>Salary Structure</th>
                            <th>Grade Level</th>
                            <th>Step</th>
                            <th>NHIS NO</th>
                            <th>NHIS Provider</th>
                            <th>Station of Deployment</th>
                            <th>PFA</th>
                            <th>Date of Retirement</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.employeegi)
                        {
                            <tr>
                                <td>@item.File_No</td>
                                <td>@item.EmployeeCode</td>
                                <td>@item.Title</td>
                                <td>@item.First_Name</td>
                                <td>@item.Middle_Name</td>
                                <td>@item.Surname</td>
                                <td>@item.Sex</td>
                                @foreach (var item1 in Model.employeeai.Where(x => x.EmployeeCode == item.EmployeeCode))
                                {
                                    { qualification = item1.Qualification3; }
                                   
                                }
                                <td>@qualification</td>
                                <td>@item.DateOfBirth</td>
                                <td>@item.PlaceOfBirth</td>
                                <td>@item.Marital_Status</td>
                                <td>@item.Maiden_Name</td>
                                <td>@item.Spouse_Name</td>
                                <td>@item.StateOfOrigin</td>
                                <td>@item.LGA Of Origin</td>
                                <td>@item.Home_Town</td>
                                <td>@item.Religion</td>
                                @foreach (var medical in Model.employeemi.Where(x => x.EmployeeCode == item.EmployeeCode))
                                {
                                    {   BloodGroup = medical.BloodGroup;
                                        BloodGenotype = medical.BloodGenotype;
                                        NhisNo = medical.NhisNo;
                                        NhisProvider = medical.NhisProvider;

                                    }

                                }
                                <td>@BloodGroup</td>
                                <td>@BloodGenotype</td>
                                <td>@item.ContactHomeAddress</td>
                                <td>@item.LastPromotionDate</td>
                                @foreach (var personal in Model.employeepi.Where(x => x.EmployeeCode == item.EmployeeCode))
                                {
                                    {
                                        address = personal.PermanentAddress;
                                        StateOfOrigin = personal.PermanentAddress;
                                        LGA = personal.PermanentAddress;
                                        MobileNo = personal.PermanentAddress;
                                        EmpEmailId = personal.PermanentAddress;
                                    }
                                   
                                }
                                <td>@address</td>
                                <td>@StateOfOrigin</td>
                                <td>@LGA</td>
                                <td>@MobileNo</td>
                                <td>@EmpEmailId</td>
                                <td>@item.FirstAppointmentLocation</td>
                                <td>@item.FirstAppointmentDate</td>
                                <td>@item.ConfirmationDate</td>
                                <td>@item.LastPromotionDate</td>
                                <td>@item.StationOfDeployment</td>
                                @foreach (var salary in Model.employeesi.Where(x => x.EmployeeCode == item.EmployeeCode))
                                {
                                    {
                                        BankType = salary.BankType;
                                        NameOfBanks = salary.NameOfBanks;
                                        BankBranch = salary.BankBranch;
                                        AccountNumber = salary.AccountNumber;
                                        AccountName = salary.AccountName;
                                        PFA= salary.PFA;
                                        RSAPinNo = salary.RSAPinNo;
                                        SalaryScale = salary.SalaryScale;
                                    }

                                }
                                <td>@BankType</td>
                                <td>@NameOfBanks</td>
                                <td>@BankBranch</td>
                                <td>@AccountNumber></td>
                                <td>@AccountName</td>
                                <td>@PFA</td>
                                <td>@RSAPinNo</td>

                                <td>@item.Cadre</td>
                                <td>@item.Programmes</td>
                                <td>@item.Unit_Research</td>
                                <td>@item.Section</td>
                               
                                <td>@SalaryScale</td>
                                <td>@item.Grade_Level</td>
                                <td>@item.Step</td>
                                @*<td>@item.Establishment/Civil Service No</td>*@
                               
                                <td>@NhisNo</td>
                                <td>@NhisProvider</td>
                                <td>@item.StationOfDeployment</td>
                                @*@foreach (var item3 in Model.employeesi.Where(x => x.EmployeeCode == item.EmployeeCode))
                                {
                                    <td>@item3.PFA</td>
                                }*@
                                <td>@PFA</td>
                                <td>@item.DateOfRetirement</td>


                                @* <td><span class="label label-success">Complete</span></td>*@
                                @*<td>Pierce Sr.</td>
                                    <td>03-10-2017</td>*@
                               
                            </tr>

                        }
                    </tbody>
                    <tfoot>
                        <tr>
                            <th>File No</th>
                            <th>Employee Code</th>
                            <th>Title</th>
                            <th>First Name</th>
                            <th>MiddleName</th>
                            <th>SurName</th>
                            <th>Sex</th>
                            <th>Qualification</th>
                            <th>Date Of Birth</th>
                            <th>Place Of Birth</th>
                            <th>Marital Status</th>
                            <th>Maiden Name</th>
                            <th>Spouse Name</th>
                            <th>State Of Origin</th>
                            <th>LGA Of Origin</th>
                            <th>Home Town</th>
                            <th>Religion</th>
                            <th>Blood Group</th>
                            <th>Geno Type</th>
                            <th>Contact Home Address</th>
                            <th>Last Promotion</th>
                            <th>Permanent Home Address</th>
                            <th>State Of Residence</th>
                            <th>LGA of Residence</th>
                            <th>Mobile No</th>
                            <th>Email</th>
                            <th>First Appointment</th>
                            <th>Date of First Appointment</th>
                            <th>Confirmation Date</th>
                            <th>Last Promotion Date</th>
                            <th>Job Town</th>
                            <th>Bank Type</th>
                            <th>Bank Name</th>
                            <th>Bank Branch</th>
                            <th>Account Number</th>
                            <th>Bank Account Name</th>
                            <th>Pension Name</th>
                            <th>Pension Pin</th>
                            <th>Cadre/Job Schedule</th>
                            <th>Programme</th>
                            <th>Unit</th>
                            <th>Section</th>
                            <th>Salary Structure</th>
                            <th>Grade Level</th>
                            <th>Step</th>
                            <th>NHIS NO</th>
                            <th>NHIS Provider</th>
                            <th>Station of Deployment</th>
                            <th>PFA</th>
                            <th>Date of Retirement</th>
                        </tr>
                    </tfoot>
                </table>

            </div>
        </div>
    </div>


</div>
