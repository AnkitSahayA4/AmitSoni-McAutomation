@using IARTAutomationApp.Models

@model  IARTAutomationApp.ViewModels.EmployeeAll

@{
    ViewBag.Title = "Reports";

}

<!-- Main content -->
<div class="content">

    <ol class="breadcrumb bc-colored bg-success m-b-3">
        <li class="breadcrumb-item"><a href="#">Reports</a></li>
        <li class="breadcrumb-item active">Employees General Information Report</li>
    </ol>

    <div class="card">
        <div class="card-body">
            <h4 class="text-black">Employees General Information Report</h4>
            <p>Export data to Copy, CSV, Excel & PDF</p>
            <div class="table-responsive">
                <table id="empgi" class="table table-bordered table-hover" data-name="cool-table">
                    <thead>
                        <tr>
                            <th>Employee Id</th>
                            <th>Name</th>
                            <th>Maiden Name</th>
                            <th>Rank</th>
                            <th>File No</th>
                            <th>Grade Level</th>
                            <th>Step</th>
                            <th>Cadre</th>
                            <th>Sex</th>
                            <th>Date Of Birth</th>
                            <th>Place of Birth</th>
                            <th>Marital Status</th>
                            <th>Spouse Name</th>
                            <th>State Of Origin</th>
                            <th>LGA</th>
                            <th>Home Town</th>
                            <th>Religion</th>
                            <th>Contact Home Address</th>
                            <th>First Appointment Date</th>
                            <th>First Appointment Location</th>
                            <th>Confirmation Date</th>                            
                            <th>Date Of Retirement</th>
                            <th>Last Promotion Date</th>
                            <th>Programme</th>
                            <th>Unit Research</th>
                            <th>Unit Services</th>
                            <th>Station Of Deployment</th>
                            <th>Section</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.employeegi)
                        {
                            <tr>
                                <td>@item.EmployeeCode</td>
                                <td>@item.Title @item.First_Name @item.Middle_Name @item.Surname</td>
                                <td>@item.Maiden_Name</td>
                                <td>@item.Rank</td>
                                <td>@item.File_No</td>
                                <td>@item.Grade_Level</td>
                                <td>@item.Step</td>
                                <td>@item.Cadre</td>
                                <td>@item.Sex</td>
                                <td>@item.DateOfBirth</td>
                                <td>@item.PlaceOfBirth</td>
                                <td>@item.Marital_Status</td>
                                <td>@item.Spouse_Name</td>
                                <td>@item.StateOfOrigin</td>
                                <td>@item.LGA</td>
                                <td>@item.Home_Town</td>
                                <td>@item.Religion</td>
                                <td>@item.ContactHomeAddress</td>
                                <td>@item.FirstAppointmentDate</td>
                                <td>@item.FirstAppointmentLocation</td>
                                <td>@item.ConfirmationDate</td>
                                <td>@item.DateOfRetirement</td>
                                <td>@item.LastPromotionDate</td>
                                <td>@item.Programmes</td>
                                <td>@item.Unit_Research</td>
                                <td>@item.Unit_Services</td>
                                <td>@item.StationOfDeployment</td>
                                <td>@item.Section</td>
                            </tr>
                        }
                    </tbody>
                    <tfoot>
                        <tr>
                            <th>Employee Id</th>
                            <th>Name</th>
                            <th>Maiden Name</th>
                            <th>Rank</th>
                            <th>File No</th>
                            <th>Grade Level</th>
                            <th>Step</th>
                            <th>Cadre</th>
                            <th>Sex</th>
                            <th>Date Of Birth</th>
                            <th>Place of Birth</th>
                            <th>Marital Status</th>
                            <th>Spouse Name</th>
                            <th>State Of Origin</th>
                            <th>LGA</th>
                            <th>Home Town</th>
                            <th>Religion</th>
                            <th>Contact Home Address</th>
                            <th>First Appointment Date</th>
                            <th>First Appointment Location</th>
                            <th>Confirmation Date</th>
                            <th>Date Of Retirement</th>
                            <th>Last Promotion Date</th>
                            <th>Programme</th>
                            <th>Unit Research</th>
                            <th>Unit Services</th>
                            <th>Station Of Deployment</th>
                            <th>Section</th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>

     
</div>
 

  
 
<script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.10.2.min.js" type="text/javascript"></script>
<script src="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/jquery-ui.min.js" type="text/javascript"></script>
<link href="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/themes/blitzer/jquery-ui.css" rel="Stylesheet" type="text/css" />

<script src="https://code.jquery.com/jquery-3.3.1.js" type="text/javascript"></script>

<script src="https://cdn.datatables.net/1.10.18/js/jquery.dataTables.min.js" type="text/javascript"></script>

<script src="https://cdn.datatables.net/buttons/1.5.2/js/dataTables.buttons.min.js" type="text/javascript"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" type="text/javascript"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js" type="text/javascript"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.html5.min.js" type="text/javascript"></script>

<script>

        $('#empgi').DataTable({

                dom: 'Bfrtip',
                buttons: [
                'copyHtml5',
                'excelHtml5',
                'csvHtml5',
                'pdfHtml5'
                ],

        });
</script>
