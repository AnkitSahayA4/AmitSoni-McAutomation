                <div class="col-12">
                    <div class="d-flex flex-wrap">
                        <div>
                            <h5>Employee Retirement</h5>
                        </div>
                        <div class="ml-auto">
                            <ul class="list-inline">
                                <li class="text-blue"> <i class="fa fa-circle"></i> Employee Retirement</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div id="earning"></div>
            </div>
        </div>
       


        <div class="col-lg-6" id="divbarchart">
            <div class="panel panel-default">
