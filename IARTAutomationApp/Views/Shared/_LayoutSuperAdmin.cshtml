@using IARTAutomationApp.Models
<!DOCTYPE html>
@RenderSection("my_script_variables", false)
@{ IARTDBNEWEntities db = new IARTDBNEWEntities();
    int empcode = 0;
    var empmail = "";
    var emppic = "";
    var empname = "";
    var Error = "";
    try
    {



        empcode = Convert.ToInt32(@Session["employeecode"]);

        ;
        empmail = (from a in db.EmployeePIs where a.EmployeeCode == empcode select a.EmpEmailId).FirstOrDefault();

        emppic = (from a in db.EmployeeGIs where a.EmployeeCode == empcode select a.EmployeePhoto).FirstOrDefault();
        empname = (from a in db.EmployeeGIs where a.EmployeeCode == empcode select a.First_Name + " " + a.Surname).FirstOrDefault();
    }
    catch (Exception ext)
    {
        Error = ext.ToString();
    }
}
