@model IARTAutomationApp.Models.CustomerMaster

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- Main content -->
<div class="content">
    <ol class="breadcrumb bc-colored bg-success m-b-3">
        <li class="breadcrumb-item"><a href="#">Super Admin</a></li>
        <li class="breadcrumb-item active">Tenant's Full Details</li>
    </ol>

    <div class="row">


        <div class="col-lg-3">

            <div class="card m-b-3">
                <div class="card-body">
                    <div class="box-body">
                        <strong>Tenant Name</strong>
                        <p class="text-muted"> @Html.DisplayFor(model => model.Name) </p>
                        <hr>
                        <strong>Tenant Email</strong>
                        <p class="text-muted"> @Html.DisplayFor(model => model.Email) </p>
                        <hr>
                        <strong>Tenant Phone</strong>
                        <p class="text-muted"> @Html.DisplayFor(model => model.Phone) </p>
                        <hr>
                        <strong>Website</strong>
                        <p> @Html.DisplayFor(model => model.Website) </p>
                        <hr>
                        <strong>Username</strong>
                        <p> @Html.DisplayFor(model => model.LoginName) </p>
                        <hr>
                        <strong>Password</strong>
                        <p> @Html.DisplayFor(model => model.Password) </p>

                        <hr />
                        <p>@Html.ActionLink("Edit", "Edit", new { id = Model.CustomerId })</p>
                    </div>
                    <!-- /.box-body -->
                </div>
            </div>
        </div>

    </div>
</div>